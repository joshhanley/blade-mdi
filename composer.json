{
    "name": "joshhanley/blade-mdi",
    "description": "Material Design Icons for Laravel Blade views.",
    "keywords": ["laravel", "php", "blade", "ui", "kit", "icons", "mdi", "material", "design", "icons"],
    "license": "MIT",
    "homepage": "https://github.com/joshhanley/blade-mdi",
    "authors": [
        {
            "name": "Josh Hanley",
            "homepage": "https://github.com/joshhanley",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "blade-ui-kit/blade-icons": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BladeMdi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BladeMdi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "BladeMdi\\BladeMdiServiceProvider"
            ]
        }
    }
}
